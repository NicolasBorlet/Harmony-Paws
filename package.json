{
  "name": "harmony-paws",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:prod": "=production start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test-watch": "jest --watch --coverage=false --changedSince=origin/main",
    "test-debug": "jest -o --watch --coverage=false",
    "test-final": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "rid": "expo run:ios --device",
    "rad": "expo run:android --device",
    "lint": "expo lint",
    "format": "prettier --write \"**/*.{ts, tsx, js, jsx, md, mdx, css, yaml, ym?}\"",
    "check": "prettier --check \"**/*.{ts, tsx, js, jsx,md, mdx, css, yaml, ym?}\"",
    "prepare": "husky",
    "maestro:all": "bash scripts/run-all-maestro-tests.sh",
    "maestro:essential": "bash scripts/run-maestro-tests.sh",
    "maestro:flow": "maestro test"
  },
  "lint-staged": {
    "*.{fjs, jsx,ts, tsx}": [
      "eslint --fix",
      "prettier -write"
    ],
    "*.{json, ess,md}": [
      "prettier --write"
    ]
  },
